{
  "name": "TabFlow",
  "description": "A beautiful and elegant new tab for your browser.",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides" : {
    "newtab": "pages/dashboard.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage"]
}